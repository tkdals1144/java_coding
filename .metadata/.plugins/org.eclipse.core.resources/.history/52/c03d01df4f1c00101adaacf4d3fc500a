package Studying;

public class Study02 {
	public static void main(String[] args) {
		Parent p = new Child();
		p.display();
	}
}

class Parent {
	static void display() {
		System.out.println("Parent static display");
	}
}

class Child extends Parent {
	static void display() {
		System.out.println("Child static display");
	}
}