package Studying;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

public class Main {
	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader bf = new BufferedReader(new InputStreamReader(System.in));
		
		String XY = bf.readLine();
		StringTokenizer st = new StringTokenizer(XY);
		
		int X = Integer.parseInt(st.nextToken());
		int Y = Integer.parseInt(st.nextToken());
		
		System.out.println(Rev(Rev(X) + Rev(Y)));
	}
	
	static int Rev(int a) {
		if (a >= 1000) {
			// 1235
			int a1 = a / 1000; // 1
			int a2 = a / 100 - a1 * 10; // 2
			int a3 = a / 10 - a2 * 10 - a1 * 100; // 3
			int a4 = a - a3 * 10 - a2 * 100 - a1 * 1000; // 5
			return a4 * 1000 + a3 * 100 + a2 * 10 + a1;
		} else if (a >= 100) {
			// 532
			int a1 = a / 100; // 5
			int a2 = a / 10 - a1 * 10; // 53 - 50 = 3
			int a3 = a - a2 * 10 - a1 * 100; // 532 - 30 - 500 = 2
			return a3 * 100 + a2 * 10 + a1;
		} else if (a >= 10) {
			// 54
			int a1 = a / 10; // 5
			int a2 = a - a1 * 10; // 54 - 50 = 4
			return a2 * 10 + a1;
		} else {
			return a;
		}
	}
}
